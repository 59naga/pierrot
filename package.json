{
  "name": "pierrot",
  "version": "0.0.0-alpha.2",
  "description": "pm2 bouncy plugin",

  "bin": "pierrot",
  "engines": {
    "node" : ">=0.12.0"
  },

  "scripts": {
    "vhost": "sudo PIERROT_CWD=$(pwd)/test/ coffee pierrot vhost",
    "apps": "PIERROT_CWD=$(pwd)/test/ coffee pierrot apps",

    "prepublish": "node -e \"if(process.env.TRAVIS){}else{process.exit(1)}\" && npm run compile || echo skip prepublish",
    "compile": "coffee --bare --compile index.coffee",
    "postcompile": "coffee --output lib --bare --compile src"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    }
  },

  "dependencies": {
    "bluebird": "^2.9.33",
    "chmodr": "^0.1.1",
    "commander": "^2.8.1",
    "inquirer": "^0.9.0",
    "js-yaml": "^3.3.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.4.1"
  },
  "devDependencies": {
    "bouncy": "^3.2.2",
    "jasminetea": "^0.2.1",
    "object-parser-cli": "0.0.1-alpha.1",
    "pm2": "^0.14.3",
    "strip-ansi": "^3.0.0"
  },
  
  "keywords": [
    "pm2",
    "pm2plugin",
    "bouncy"
  ],
  "author": "59naga <i59naga@icloud.com> (http://berabou.me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/59naga/pierrot.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/pierrot/issues"
  },
  "homepage": "https://github.com/59naga/pierrot#readme"
}
